*saveLines: 65535

! rxvt in Windows, scrollbar
rxvt.scrollBar: true
rxvt.scrollBar_right: true

! URxvt in Linux, no scrollbar
URxvt.scrollBar: false

rxvt.scrollTtyOutput: false
rxvt.scrollTtyKeypress: true
rxvt.scrollWithBuffer: true

URxvt.secondaryScroll: true

URxvt.font: xft:terminus:size=12:antialias=true
URxvt.boldFont: xft:terminus:bold:size=12:antialias=true
!rxvt.font: consolas-16
!rxvt.boldFont: consolas-16

URxvt.perl-ext-common: default,matcher
URxvt.url-launcher: /usr/bin/xdg-open
URxvt.matcher.button: 1

! Disable annoying iso14755 mode triggering on Ctrl+Shift
URxvt.iso14755: false
URxvt.iso14755_52: false

URxvt.keysym.C-Tab:	\033<C-Tab>

